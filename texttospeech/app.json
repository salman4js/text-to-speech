{
  "expo": {
    "name": "Text-Speech",
    "slug": "texttospeech",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/tech-support.png",
    "splash": {
      "image": "./assets/splash1.png",
      "resizeMode": "cover",
      "backgroundColor": "#009688"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.azeemstark.texttospeech"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
